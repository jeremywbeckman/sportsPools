<script type="text/ng-template" id="/manageLeagues.html">
   <div class="page-header">
      <h1>League Management</h1>
   </div>
   <div ng-show="error.message" class="alert alert-danger row">
      <span>{{ error.message }}</span>
   </div>
   <div>
      <h3>Review League Names ({{reviewLeagues.length}})</h3>
   </div>
   <form style="margin-top:20px;" name="LeagueReviewForm" ng-submit="approveLeague()">
      <div class="form-group row">
         <input type="text"
                class="form-control col-md-10"
                ng-model="goodLeague"/>
         <button type="submit" class="btn btn-primary">Submit</button>
      </div>
   </form>
</script>